{
    "info": {
      "name": "WhatsApp NestJS API",
      "_postman_id": "example-collection-id",
      "description": "Collection for WhatsApp NestJS API",
      "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
      {
        "name": "Send WhatsApp Message",
        "request": {
          "method": "POST",
          "header": [
            {
              "key": "Content-Type",
              "value": "application/json"
            }
          ],
          "body": {
            "mode": "raw",
            "raw": "{\n  \"to\": \"1234567890@c.us\",\n  \"message\": \"Hello from NestJS!\"\n}"
          },
          "url": {
            "raw": "http://localhost:3000/whatsapp/send",
            "protocol": "http",
            "host": [
              "localhost"
            ],
            "port": "3000",
            "path": [
              "whatsapp",
              "send"
            ]
          }
        },
        "response": []
      }
    ]
  }
  